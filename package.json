{
  "name": "fryge-rss-sync",
  "version": "0.0.1",
  "author": {
    "name": "Daniel Goldshlack",
    "email": "daniel@goldshlack.net"
  },
  "description": "Use this to create and maintain a local copy of all data from a set of rss feeds.",
  "repository": "https://github.com/dangoldnj/fryge-rss-sync",
  "license": "MIT",
  "scripts": {
    "start": "node server.js",
    "test": "xo"
  },
  "dependencies": {
    "davefeedread": "^0.5.14",
    "isomorphic-fetch": "^3.0.0",
    "node-fs": "^0.1.7"
  },
  "devDependencies": {
    "xo": "^0.21.1"
  },
  "xo": {
    "rules": {
      "comma-dangle": [
        "error",
        "always-multiline"
      ],
      "object-curly-spacing": [
        "error",
        "always"
      ],
      "prefer-promise-reject-errors": 0,
      "template-curly-spacing": [
        "error",
        "always"
      ],
      "unicorn/filename-case": "camelCase"
    },
    "space": true
  }
}
